{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    // 数据库连接字符串。Aspire 会在运行时自动覆盖这个值
    // JWT 配置节
    "Jwt": {
        "Issuer": "https://authservice.chatapp.com", // 令牌颁发者
        "Audience": "https://api.chatapp.com", // 令牌受众 (通常是你的 API 网关)
        "SecretKey": "THIS_IS_A_SUPER_SECRET_KEY_FOR_JWT_32_CHARS_LONG"
    }
}